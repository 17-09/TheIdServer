<ul class="list-inline">
    @foreach (var claim in Claims)
    {
        <li @key="claim" class="list-inline-item">
            <ClaimType Entity="@claim"
                       DeleteClicked="() => OnDeleteClaimClicked(claim)" />
        </li>
    }
    <li class="list-inline-item">
        <ClaimType Entity="@_claim"
                   ValueChanged="() => OnClaimValueChanged(_claim)" />
    </li>
</ul>
